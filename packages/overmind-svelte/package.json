{
  "name": "overmind-svelte",
  "version": "1.0.0-alpha1",
  "description": "Functional actions",
  "author": "Marcus Feitoza <marcus.feitoza.filho@gmail.com>",
  "license": "MIT",
  "repository": "git+https://github.com/cerebral/overmind.git",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:lib & npm run build:es",
    "build:lib": "tsc --outDir lib --module commonjs",
    "build:es": "tsc --outDir es --module ES2020 --target ES2020",
    "clean": "rimraf es lib coverage",
    "typecheck": "tsc --noEmit",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --updateSnapshot --coverage false",
    "prebuild": "npm run clean",
    "postbuild": "rimraf {lib,es}/**/__tests__",
    "posttest": "npm run typecheck"
  },
  "keywords": [
    "state",
    "sideeffects",
    "app",
    "framework"
  ],
  "files": [
    "lib",
    "es",
    "react"
  ],
  "dependencies": {
    "overmind": "next",
    "svelte": "^3.59.2",
		"tslib": "^2.6.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/svelte": "^3.2.2",
    "@types/node": "^20.5.9",
    "svelte-jester": "^2.3.2"
  }
}
