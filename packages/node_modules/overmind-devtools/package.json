{
	"name": "overmind-devtools",
	"version": "1.0.0-alpha1",
	"description": "Overmind devtool",
	"author": "Christian Alfoni <christianalfoni@gmail.com>",
	"license": "MIT",
	"repository": "git+https://github.com/cerebral/overmind.git",
	"bin": {
		"overmind-devtools": "bin.js"
	},
	"scripts": {
		"start": "concurrently \"electron ./backend/main\" \"webpack-dev-server --port 3000\"",
		"build": "webpack --mode=production",
		"typecheck": "tsc --noEmit",
		"test": "jest --runInBand",
		"test:watch": "jest --watch --updateSnapshot --coverage false",
		"prepare": "npm run build",
		"posttest": "npm run typecheck"
	},
	"keywords": [
		"overmind",
		"devtool"
	],
	"dependencies": {
		"@types/node": "^10.5.1",
		"color": "^3.0.0",
		"color-hash": "^1.0.3",
		"electron": "^2.0.8",
		"electron-json-storage": "^4.1.0",
		"overmind-react": "next",
		"react": "16.7.0-alpha.2",
		"react-dom": "16.7.0-alpha.2",
		"overmind-themes": "next",
		"emotion": "^9.2.12",
		"ws": "^5.2.1"
	},
	"devDependencies": {
		"@types/jest": "^23.1.4",
		"@types/react": "^16.7.13",
		"jest": "^23.5.0",
		"terser-webpack-plugin": "^1.2.1",
		"ts-jest": "^23.10.4",
		"ts-loader": "^4.4.2",
		"tslib": "^1.9.3",
		"typescript": "^3.1.3",
		"url-loader": "^1.0.1",
		"webpack": "4.28.4",
		"webpack-cli": "^3.1.2"
	}
}
