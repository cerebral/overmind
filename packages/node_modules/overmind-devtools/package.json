		"electron-json-storage": "^4.1.0",
    "overmind-react": "next",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "overmind-themes": "next",
    "emotion": "^9.2.12",
		"ws": "^5.2.1"
	}
