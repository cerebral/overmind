{
	"name": "overmind-devtools-client",
	"version": "1.0.0-alpha1",
	"description": "Overmind devtool",
	"author": "Christian Alfoni <christianalfoni@gmail.com>",
	"license": "MIT",
	"repository": "git+https://github.com/cerebral/overmind.git",
	"scripts": {
		"start": "concurrently \"npm run start:ws\" \"npm run start:devtools\"",
		"start:ws": "node server.js",
		"start:devtools": "OV_DEV_PORT=$OV_DEV_PORT webpack-dev-server",
		"build": "webpack --config webpack.prod.js",
		"typecheck": "tsc --noEmit --skipLibCheck",
		"prepare": "npm run build"
	},
	"keywords": [
		"overmind",
		"devtool"
	],
	"dependencies": {
		"@babel/runtime": "^7.28.4",
		"color-hash": "^2.0.2",
		"emotion": "^9.2.12",
		"lodash.clonedeep": "^4.5.0",
		"overmind-react": "next",
		"react": "^19.2.0",
    	"react-dom": "^19.2.0",
		"tslib": "^2.8.1",
		"ws": "^8.18.3"
	},
	"devDependencies": {
    	"@babel/plugin-transform-class-properties": "^7.27.1",
		"babel-loader": "^10.0.0",
		"concurrently": "^9.2.1",
		"html-webpack-plugin": "^5.6.4",
		"url-loader": "^4.1.1",
		"terser-webpack-plugin": "^5.3.14",
		"webpack": "^5.102.1",
		"webpack-dev-server": "^5.2.2",
		"webpack-merge": "^6.0.1"
	}
}
