{
  "name": "overmind-monorepo",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "repo-cooker --build",
    "checkdeps": "repo-cooker --check-dependencies",
    "commit": "git-cz",
    "commit-msg": "cz-customizable-ghooks",
    "fixdeps": "repo-cooker --fix-dependencies",
    "format": "eslint --fix . && prettier --write --list-different .",
    "install": "npm run link && npm run build",
    "link": "repo-cooker --link",
    "lint": "eslint . && prettier --check .",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "release": "repo-cooker --release",
    "test": "repo-cooker test --no-parallel",
    "typecheck": "repo-cooker typecheck"
  },
  "heroku-run-build-script": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebral/overmind.git"
  },
  "dependencies": {
    "@babel/runtime": "7.28.4",
    "color-hash": "2.0.2",
    "electron": "35.7.5",
    "electron-json-storage": "4.6.0",
    "emotion": "9.2.12",
    "graphql": "16.11.0",
    "graphql-request": "5.1.0",
    "graphql-tag": "2.12.6",
    "is-plain-obj": "3.0.0",
    "lodash.clonedeep": "4.5.0",
    "phoenix": "1.8.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "5.5.0",
    "svelte": "3.59.2",
    "tslib": "2.8.1",
    "vue": "3.5.22",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-class-properties": "7.27.1",
    "@babel/plugin-transform-object-rest-spread": "^7.28.4",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.38.0",
    "@size-limit/file": "11.2.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/svelte": "3.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/phoenix": "^1.6.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/vscode": "^1.70.0",
    "@types/ws": "^8.18.1",
    "@vscode/vsce": "3.6.2",
    "babel-loader": "10.0.0",
    "commitizen": "^4.3.1",
    "concurrently": "9.2.1",
    "cz-customizable": "^7.5.1",
    "cz-customizable-ghooks": "^2.0.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "html-webpack-plugin": "5.6.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "16.2.6",
    "prettier": "^3.6.2",
    "repo-cooker": "^8.0.23",
    "rimraf": "^6.0.1",
    "size-limit": "11.2.0",
    "svelte-jester": "2.3.2",
    "terser-webpack-plugin": "5.3.14",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "url-loader": "4.1.1",
    "webpack": "5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "5.2.2",
    "webpack-merge": "6.0.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,css,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  }
}
