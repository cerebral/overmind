{
  "name": "overmind-monorepo",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "repo-cooker --build",
    "checkdeps": "repo-cooker --check-dependencies",
    "commit": "git-cz",
    "commitmsg": "cz-customizable-ghooks",
    "fixdeps": "repo-cooker --fix-dependencies",
    "format": "npm run lint -- --fix && prettier --write --with-node-modules \"**/*.md\" \"**/*.css\" \"**/*.json\"",
    "install": "npm run link && npm run build",
    "link": "repo-cooker --link",
    "lint": "eslint \"**/*.js\" \"**/*.ts\"",
    "precommit": "lint-staged",
    "release": "repo-cooker --release",
    "test": "repo-cooker test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebral/overmind.git"
  },
  "dependencies": {
    "@types/node": "10.5.1",
    "color": "3.0.0",
    "electron": "2.0.4",
    "electron-json-storage": "4.1.0",
    "is-plain-object": "2.0.4",
    "parcel": "^1.9.7",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "styled-components": "3.3.3",
    "tslib": "1.9.3",
    "ws": "5.2.1"
  },
  "devDependencies": {
    "@types/jest": "23.1.4",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "commitizen": "^2.10.1",
    "concurrently": "^3.6.0",
    "cz-customizable": "^5.2.0",
    "cz-customizable-ghooks": "^1.5.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-typescript": "^0.12.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "23.3.0",
    "lint-staged": "^7.2.0",
    "prettier": "1.13.7",
    "react": "16.4.1",
    "react-test-renderer": "16.4.1",
    "repo-cooker": "^6.2.5",
    "rimraf": "2.6.2",
    "ts-jest": "23.0.0",
    "ts-loader": "4.4.2",
    "tslib": "1.9.3",
    "typescript": "2.9.2",
    "typescript-eslint-parser": "^16.0.1",
    "webpack": "4.15.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "^3.1.4",
    "url-loader": "1.0.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,css,json}": [
      "prettier --write --with-node-modules",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  }
}
